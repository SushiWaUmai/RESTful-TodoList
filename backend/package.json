    "@types/node": "^16.9.6",
    "@types/nodemailer": "^6.4.4",
    "copyfiles": "^2.4.1",
    "cross-var": "^1.1.0",
    "dotenv-cli": "^4.0.0",
    "kill-port": "^1.6.1",
    "nodemon": "^2.0.12",
    "rimraf": "^3.0.2",
    "ts-node": "^10.2.1",
    "typescript": "^4.4.3"
  }
