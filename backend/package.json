{
  "name": "restful-todolist-backend",
  "version": "0.1.0",
  "description": "",
  "main": "dist/backend/src/index.js",
  "scripts": {
    "dev": "yarn clean && nodemon --exec yarn build",
    "build": "yarn kill && tsc && yarn css && yarn copy && yarn start",
    "cleanbuild": "yarn clean && yarn build",
    "clean": "rimraf dist",
    "kill": "dotenv cross-var kill-port %PORT%",
    "start": "node dist/backend/src",
    "copy": "copyfiles -u 1 src/templates/* dist/backend/src",
    "css": "tailwindcss -o src/templates/style.css"
  },
  "keywords": [],
  "author": "Eugene Matsumura",
  "license": "MIT",
  "_moduleAliases": {
    "src": "./dist/backend/src/",
    "@shared": "./dist/shared/src/"
  },
  "dependencies": {
    "@typegoose/typegoose": "^9.0.0",
    "argon2": "^0.28.2",
    "autoprefixer": "^10.3.6",
    "connect-mongo": "^4.6.0",
    "cors": "^2.8.5",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "express-session": "^1.17.2",
    "hogan.js": "^3.0.2",
    "juice": "^8.0.0",
    "module-alias": "^2.2.2",
    "mongodb": "^4.1.2",
    "mongoose": "^6.0.7",
    "morgan": "^1.10.0",
    "nodemailer": "^6.6.5",
    "postcss": "^8.3.8",
    "tailwindcss": "^2.2.16"
